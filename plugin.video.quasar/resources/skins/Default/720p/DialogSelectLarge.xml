<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog">
	<!-- 1280 x 720 -->
	<defaultcontrol always="true">32503</defaultcontrol>
	<include>dialogeffect</include>
	<depth>DepthDialog+</depth>
  <controls>
		<control type="group">
			<control type="image">
				<!-- Background -->
				<posx>190</posx>
				<posy>60</posy>
				<width>900</width>
				<height>600</height>
				<texture>AddonWindow/dialog_back.png</texture>
			</control>
			<control type="button" id="32500">
				<!-- Close button -->
				<posx>1028</posx>
				<posy>82</posy>
				<width>32</width>
				<height>32</height>
				<texturefocus>AddonWindow/dialog_close_focus.png</texturefocus>
				<texturenofocus>AddonWindow/dialog_close_nofocus.png</texturenofocus>
				<onup>32508</onup>
				<ondown>32503</ondown>
			</control>
			<control type="label" id="32501">
				<!-- Window title -->
				<posx>290</posx>
				<posy>107</posy>
				<width>700</width>
				<height>30</height>
				<font>font14</font>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>silver</textcolor>
				<shadowcolor>AA000000</shadowcolor>
			</control>
			<control type="scrollbar" id="32502">
				<!-- Scrollbar -->
				<posx>1052</posx>
				<posy>142</posy>
				<width>10</width>
				<height>470</height>
				<onleft>32503</onleft>
				<onup>32500</onup>
