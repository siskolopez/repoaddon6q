				<focusposition>0</focusposition>
				<pagecontrol>32502</pagecontrol>
				<scrolltime>200</scrolltime>
				<preloaditems>2</preloaditems>
				<itemlayout height="65" width="790">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>790</width>
						<height>60</height>
						<texture border="5">Button/button-nofocus.png</texture>
					</control>
					<control type="image">
						<posx>5</posx>
						<posy>5</posy>
						<width>50</width>
						<height>50</height>
						<texture>$INFO[ListItem.Icon]</texture>
					</control>
					<control type="label">
						<posx>70</posx>
						<posy>2</posy>
						<width>707</width>
						<height>30</height>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>silver</textcolor>
						<shadowcolor>AA000000</shadowcolor>
