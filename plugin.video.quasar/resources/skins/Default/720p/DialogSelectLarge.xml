						<posx>70</posx>
						<posy>28</posy>
						<width>707</width>
						<height>30</height>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<shadowcolor>AA000000</shadowcolor>
						<info>ListItem.Label2</info>
					</control>
				</focusedlayout>
			</control>
		</control>
  </controls>
</window>
